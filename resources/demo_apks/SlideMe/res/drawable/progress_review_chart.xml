<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#566f7074" android:endColor="#568f9094" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <inset android:insetLeft="1.0dip" android:insetRight="1.0dip" android:insetTop="1.0dip" android:insetBottom="1.0dip">
                <shape>
                    <gradient android:startColor="#ffab0a51" android:endColor="#ffff3f94" android:angle="90.0" />
                </shape>
            </inset>
        </clip>
    </item>
</layer-list>